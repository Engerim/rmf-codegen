#%RAML 1.0
title: property query parameter placeholder annotation rule

/products:
    get:
        queryParameters:
            /text.[a-z]{2}/?:
                (placeholderParam):
                    paramName: text
                    template: text.<locale>
                    placeholder: locale
                type: string
            filter:
                type: string
            /invalid/?:
                type: string
            /incomplete.[a-z]{2}/?:
                (placeholderParam):
                    paramName: text
                type: string

