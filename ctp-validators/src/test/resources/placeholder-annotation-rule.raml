#%RAML 1.0
title: property query parameter placeholder annotation rule

/products:
    get:
        queryParameters:
            /text.en/?:
                (placeholderParam):
                    paramName: text
                    template: text.<locale>
                    placeholder: locale
                type: string
            /text.eng/?:
                (placeholderParam):
                    paramName: text
                    template: text.<locale>
                    placeholder: locale
                type: string

