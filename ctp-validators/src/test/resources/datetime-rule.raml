    type: object
    properties:
      fooDateTime: datetime
  InvalidDate:
    type: object
    properties:
      fooDate: date-only
  InvalidTime:
    type: object
    properties:
      fooTime: time-only
  InvalidDateTimeRangeFrom:
    type: object
    properties:
      fooDateTimeFrom: datetime
  InvalidDateTimeRangeTo:
    type: object
    properties:
      fooDateTimeTo: datetime
  InvalidDateRangeFrom:
    type: object
    properties:
      fooDateFrom: date-only
  InvalidDateRangeTo:
    type: object
    properties:
      fooDateTo: date-only
  InvalidTimeRangeFrom:
    type: object
    properties:
      fooTimeFrom: time-only
  InvalidTimeRangeTo:
    type: object
    properties:
      fooTimeTo: time-only
  FooAtDateTime:
    type: object
    properties:
      fooAt: datetime
  FooRangeDateTime:
    type: object
    properties:
      fooFrom: datetime
      fooTo: datetime
  FooAtDate:
    type: object
    properties:
      fooAt: date-only
  FooRangeDate:
    type: object
